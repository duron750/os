--- Makefile
+++ Makefile	1994/02/09 16:32:33
@@ -0,0 +1,14 @@
+CFLAGS = -O2 -fomit-frame-pointer -m486 \
+		-include /usr/include/bsd/bsd.h -I/usr/include/bsd
+LDLIBS = -lbsd
+LDFLAGS = -s -v
+
+rwall: rwall.o
+
+install: rwall
+	install -m755 rwall /usr/bin
+	install -m644 rwall.1 /usr/man/man1
+
+clean:
+	rm -f *.o rwall
+
